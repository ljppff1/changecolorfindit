<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.manuelpeinado.fadingactionbar.demo"
    android:versionCode="312"
    android:versionName="3.1.2" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="19" />

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_location_map"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.manuelpeinado.fadingactionbar.demo.TestActivity"
            android:icon="@drawable/ic_location_map"
            android:label="@string/choose_city"
            android:theme="@style/AppTheme.TranslucentActionBar" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>